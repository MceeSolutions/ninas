<odoo>
    <data>
    
    	<record id="stock_location_employee" model="stock.location">
            <field name="name">Employees</field>
            <field name="location_id" ref="stock.stock_location_locations_partner"/>
            <field name="usage">customer</field>
            <field name="company_id"></field>
        </record>   
        
       <record id="stock_picking_type_emp" model="stock.picking.type">
            <field name="name">Staff Store Requests</field>
            <field name="code">internal</field>
            <field name="use_create_lots">0</field>
            <field name="use_existing_lots">1</field>
            <field name="default_location_src_id" ref="stock_location_employee"/>
            <field name="default_location_dest_id" ref="stock_location_employee"/>
            <field name="sequence_id">20</field>
        </record>

        <record id="seq_customer_code" model="ir.sequence">
        	<field name="name">Travel Request ID</field>
        	<field name="code">travel.request</field>
        	<field name="prefix">TR</field>
        	<field name="padding">3</field>
        </record>
        
        <record id="seq_employee_id" model="ir.sequence">
        	<field name="name">Employee ID</field>
        	<field name="code">hr.employee</field>
        	<field name="padding">5</field>
        </record>
        
        <record id="seq_reference_number" model="ir.sequence">
        	<field name="name">Reference Number</field>
        	<field name="code">ninas.assessment_plan</field>
			<field name="prefix">AP</field>
        	<field name="padding">3</field>
        </record>

        <record id="payment_icon_cc_verve" model="payment.icon">
            <field name="name">Verve</field>
            <field name="image" type="base64" file="payment_gtpay/static/src/img/verve.png"/>
        </record>

        <record id="payment_icon_cc_bank_transfer" model="payment.icon">
            <field name="name">Bank Transfer</field>
            <field name="image" type="base64" file="payment_gtpay/static/src/img/bank_transfer.png"/>
        </record>

        <record id="payment_icon_cc_ussd" model="payment.icon">
            <field name="name">USSD</field>
            <field name="image" type="base64" file="payment_gtpay/static/src/img/ussd.png"/>
        </record>

        <record id="payment_acquirer_gtpay" model="payment.acquirer">
        <field name="name">GTPay</field>
        <field name="sequence">1</field>
        <field name="view_template_id" ref="payment_gtpay.submit2gtpay_form"/>
        <field name="image" type="base64" file="payment_gtpay/static/src/img/gtpay_icon.png"/>
        <field name="module_id" ref="base.module_payment_gtpay"/>
        <field name="description" type="html">
            <p>
                GTPay is a secure internet payment gateway developed to facilitate payments online using debit Cards issued by banks on the Interswitch Network.
            </p>
            <ul class="list-inline">
                <li><i class="fa fa-check"/>Online Payment</li>
                <li><i class="fa fa-check"/>Payment Status Tracking</li>
            </ul>
        </field>
        <!-- https://www.gtbank.com/business-banking/sme-banking/e-commerce-solutions/gtpay -->
        <field name="payment_icon_ids" eval='[(6, 0, [ref("payment.payment_icon_cc_mastercard"),
                                                      ref("payment.payment_icon_cc_visa"),
                                                      ref("payment_icon_cc_verve"),
                                                      ref("payment_icon_cc_bank_transfer"),
                                                      ref("payment_icon_cc_ussd")])]'/>
    </record>
    
    
    </data>
</odoo>